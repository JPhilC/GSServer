            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="120"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <TextBlock Grid.Row="0" Grid.Column="0" Margin="0,10,0,0" Text="{StaticResource botSlewing}" HorizontalAlignment="Center"  FontSize="30">
                <TextBlock.Style>
                    <Style TargetType="TextBlock" BasedOn="{StaticResource MaterialDesignTextBlock}">
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding IsSlewing}" Value="True">
                                <Setter Property="Foreground" Value="{StaticResource SecondaryHueMidBrush}"></Setter>
                            </DataTrigger>
                            <DataTrigger Binding="{Binding IsSlewing}" Value="False">
                                <Setter Property="Foreground" Value="Transparent" />
                            </DataTrigger>
                            <Trigger Property="IsEnabled" Value="False">
                                <Setter Property="Opacity" Value=".5" />
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </TextBlock.Style>
            </TextBlock>
            <StackPanel Grid.Row="0" Grid.Column="0">
                <Button Height="25" Padding="-5" ToolTip="{StaticResource 1021In}" Margin="5" Content="{StaticResource 1021In}" Style="{StaticResource MaterialDesignRaisedDarkButton}" Command="{Binding SpiralInCmd }" IsEnabled="{Binding IsSlewing, Converter={StaticResource InvBol}}"/>
                <Button Height="25" Padding="-5" ToolTip="{StaticResource 1021Out}" Margin="5" Content="{StaticResource 1021Out}" Style="{StaticResource MaterialDesignRaisedDarkButton}" Command="{Binding SpiralOutCmd }" IsEnabled="{Binding IsSlewing, Converter={StaticResource InvBol}}"/>
                <Button Height="25" Padding="-5" ToolTip="{StaticResource 1021Abort}" Margin="5" Content="{StaticResource 1021Abort}" Style="{StaticResource MaterialDesignRaisedDarkButton}" Command="{Binding SpiralStopCmd }" />
                <Button Height="25" Padding="-5" ToolTip="{StaticResource 1021New}" Margin="5" Content="{StaticResource 1021New}" Style="{StaticResource MaterialDesignRaisedDarkButton}" Command="{Binding SpiralGenerateCmd }" IsEnabled="{Binding IsSlewing, Converter={StaticResource InvBol}}" />
                <Button Height="25" Padding="-5" ToolTip="{StaticResource 1021Clear}" Margin="5" Content="{StaticResource 1021Clear}" Style="{StaticResource MaterialDesignRaisedDarkButton}" Command="{Binding SpiralClearCmd }" IsEnabled="{Binding IsSlewing, Converter={StaticResource InvBol}}"/>
                <Button Height="25" Padding="-5" ToolTip="{StaticResource 1021Calculate}" Margin="5" Content="{StaticResource 1021Calculate}" Style="{StaticResource MaterialDesignRaisedDarkButton}" Command="{Binding OpenSpiralFovDialogCmd }" />
                <TextBox md:HintAssist.Hint="{StaticResource 1021WidthA}" Width="100" ToolTip="{StaticResource 1021WidthA}" HorizontalAlignment="Left" Margin="5,0,0,0" Style="{StaticResource MaterialDesignFloatingHintTextBox}">
                    <TextBox.Text>
                        <Binding Path="Width" Mode="TwoWay" UpdateSourceTrigger="LostFocus">
                            <Binding.ValidationRules>
                                <domain:NotEmptyValidationRule ValidatesOnTargetUpdated="True" />
                                <domain1:IntValidationRule ValidatesOnTargetUpdated="True" />
                            </Binding.ValidationRules>
                        </Binding>
                    </TextBox.Text>
                </TextBox>
                <TextBox md:HintAssist.Hint="{StaticResource 1021HeightA}" Width="100" ToolTip="{StaticResource 1021HeightA}" HorizontalAlignment="Left" Margin="5,5,0,0" Style="{StaticResource MaterialDesignFloatingHintTextBox}">
                    <TextBox.Text>
                        <Binding Path="Height" Mode="TwoWay" UpdateSourceTrigger="LostFocus">
                            <Binding.ValidationRules>
                                <domain:NotEmptyValidationRule ValidatesOnTargetUpdated="True" />
                                <domain1:IntValidationRule ValidatesOnTargetUpdated="True" />
                            </Binding.ValidationRules>
                        </Binding>
                    </TextBox.Text>
                </TextBox>
                <StackPanel Margin="5,20,0,0" Orientation="Horizontal">
                    <TextBlock Text="{StaticResource 1021LimitsOn}" VerticalAlignment="Center"  />
                    <ToggleButton HorizontalAlignment="Center" Width="25" Height="25" Margin="10,0,0,0" Style="{StaticResource MaterialDesignActionLightToggleButton}" ToolTip="{StaticResource 1021LimitsOn}" IsChecked="{Binding SpiralLimits, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                </StackPanel>
            </StackPanel>
                <Border Grid.Column="1" BorderBrush="Black" BorderThickness="1">
                <Grid x:Name="SpiralGrid" Margin="5" ShowGridLines="False">
                    <Grid.Background>
                        <SolidColorBrush  Opacity="0.1" />
                    </Grid.Background>
                    <Grid.RowDefinitions>
                    <RowDefinition MinHeight="20" />
                    <RowDefinition MinHeight="20" />
                    <RowDefinition MinHeight="20" />
                    <RowDefinition MinHeight="20" />
                    <RowDefinition MinHeight="20" />
                    <RowDefinition MinHeight="20" />
                    <RowDefinition MinHeight="20" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition MinWidth="20" />
                    <ColumnDefinition MinWidth="20" />
                    <ColumnDefinition MinWidth="20" />
                    <ColumnDefinition MinWidth="20" />
                    <ColumnDefinition MinWidth="20" />
                    <ColumnDefinition MinWidth="20" />
                    <ColumnDefinition MinWidth="20" />
                </Grid.ColumnDefinitions>
                    <md:DialogHost Grid.Row="0" Grid.Column="0" Grid.RowSpan="7" Grid.ColumnSpan="7" CloseOnClickAway="True" IsOpen="{Binding IsSpiralFovDialogOpen}" DialogContent="{Binding SpiralFovContent}" />
                    <md:DialogHost Grid.Row="0" Grid.Column="0" Grid.RowSpan="7" Grid.ColumnSpan="7" CloseOnClickAway="True" IsOpen="{Binding IsDialogOpen}" DialogContent="{Binding DialogContent}" />
                    <md:DialogHost Grid.Row="0" Grid.Column="0" Grid.RowSpan="7" Grid.ColumnSpan="7" CloseOnClickAway="True" IsOpen="{Binding IsSpiralGoToDialogOpen}" DialogContent="{Binding SpiralGoToContent}" />
                    <TextBlock Grid.Row="0" Grid.Column="0" Grid.RowSpan="7" HorizontalAlignment="Left"  VerticalAlignment="Center" Text="W"/>
                    <TextBlock Grid.Row="0" Grid.Column="6" Grid.RowSpan="7" HorizontalAlignment="Right"  VerticalAlignment="Center" Text="E" />
                    <TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="7" HorizontalAlignment="Center"  VerticalAlignment="Top"  Text="N" />
                    <TextBlock Grid.Row="6" Grid.Column="0" Grid.ColumnSpan="7" HorizontalAlignment="Center" VerticalAlignment="Bottom"  Text="S" />
                    <Polyline x:Name="SpiralLine" Grid.Row="0" Grid.RowSpan="7" Grid.ColumnSpan="7" Grid.Column="0" Stroke="{StaticResource SecondaryHueMidBrush}" StrokeThickness="2" />
                    <StackPanel Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="5" Orientation="Horizontal" VerticalAlignment="Top" Opacity=".4">
